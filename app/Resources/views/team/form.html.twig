<div class="modal" id="{{ formId }}">
    <a href="#" class="close">&times;</a>
    <div class="modal-header">{{ title }}</div>
    <div class="modal-body">
       {{ form_start(form) }}
        {{ form_row(form) }}
        <button type="submit" class="button">Envoyer</button>
        {{ form_end(form) }}
    </div>
</div>